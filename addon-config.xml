<!-- 
  use this addon-config.xml to create/set/publish your property tree
  or override default settings from defaults.xml

  Node: this file MUST be named addon-config.xml and it MUST be in the
  root path of your addon/plugin.
  The topmost element MUST be <PropertyList>
-->

<PropertyList>
    <addons>
        <by-id>
            <org.flightgear.addons.rampmarshall>
                <addon-devel>
                    <enable type="bool">1</enable>
                </addon-devel>
				<dialog>
					<settings>
						<marshall-x-pos type="double">0.0</marshall-x-pos>
						<marshall-y-pos type="double">0.0</marshall-y-pos>
						<marshall-z-pos type="double">0.0</marshall-z-pos>
						<marshall-heading type="double">0.0</marshall-heading>
						<marshall-x-pos-temp type="double">0.0</marshall-x-pos-temp>
						<marshall-y-pos-temp type="double">0.0</marshall-y-pos-temp>
						<marshall-z-pos-temp type="double">0.0</marshall-z-pos-temp>
						<marshall-heading-temp type="double">0.0</marshall-heading-temp>
					</settings>
				</dialog>
            </org.flightgear.addons.rampmarshall>
        </by-id>
    </addons>
</PropertyList>